{
  "expo": {
    "name": "manis",
    "slug": "manis",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "manis",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": {
        "dark": "./assets/images/ios-dark.png",
        "light": "./assets/images/ios-light.png",
        "tinted": "./assets/images/ios-tinted.png"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.forimanizzati.manis"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#F2F2EA",
        "foregroundImage": "./assets/images/android-icon-foreground.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.forimanizzati.manis"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#e2296f",
          "dark": {
            "backgroundColor": "#e2296f"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/clash/otf/ClashDisplay-Regular.otf",
            "./assets/fonts/clash/otf/ClashDisplay-Light.otf",
            "./assets/fonts/clash/otf/ClashDisplay-Extralight.otf",
            "./assets/fonts/clash/otf/ClashDisplay-Medium.otf",
            "./assets/fonts/clash/otf/ClashDisplay-Semibold.otf",
            "./assets/fonts/clash/otf/ClashDisplay-Bold.otf"
          ]
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "This app needs access to your photo library to let you select images.",
          "cameraPermission": "This app needs access to your camera to let you take photos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
